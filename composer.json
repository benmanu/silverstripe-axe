{
    "name": "benmanu/axe",
    "type": "silverstripe-axe",
    "description": "Accessibility auditor for CMS users",
    "license": "BSD-3-Clause",
    "keywords": ["silverstripe","accessibility"],
    "authors": [
        {
            "name": "Ben Manu",
            "email": "ben@silverstripe.com"
        },
        {
            "name": "Sacha Judd",
            "email": "sjudd@silverstripe.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/cms": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\Axe\\": "src/",
            "SilverStripe\\Axe\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
